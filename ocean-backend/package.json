{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "create-admin": "medusa exec ./src/scripts/create-admin.ts",
    "start": "medusa start",
    "start:pm2": "pm2-runtime start ecosystem.config.js",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.10.3",
    "@medusajs/cli": "2.10.3",
    "@medusajs/framework": "2.10.3",
    "@medusajs/js-sdk": "^2.10.3",
    "@medusajs/medusa": "2.10.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@vymalo/medusa-printful": "^1.0.10",
    "awilix": "^8.0.1",
    "pg": "^8.13.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.10.3",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "jest": "^30.1.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.4.19",
    "yalc": "^1.0.0-pre.53"
  },
  "overrides": {
    "axios": "^1.7.7",
    "esbuild": "^0.25.9",
    "on-headers": "^1.1.0",
    "braces": "^3.0.3",
    "ws": "^8.18.0",
    "path-to-regexp": "0.1.10",
    "cross-spawn": "^7.0.6",
    "node-forge": "^1.3.1"
  },
  "engines": {
    "node": ">=20"
  }
}
